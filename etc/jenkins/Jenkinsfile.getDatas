pipeline {
    agent { docker { image 'python:3.7.2' } }
    stages {
        stage('Buid env') {
            steps {
                sh 'pip install --upgrade pip  && pip install -r etc/jenkins/requirements_jenkins.txt'
            }
        }
        stage('get datas') {
            steps {
                sh 'jupyter nbconvert --execute --ExecutePreprocessor.enabled=True --ExecutePreprocessor.timeout=50000 decision/PLP_ML/PipelineGetRawData.ipynb'
                sh 'jupyter nbconvert --execute --ExecutePreprocessor.enabled=True --ExecutePreprocessor.timeout=50000 decision/PLP_ML/PipelineDeRrrrafinnage.ipynb'
                sh 'jupyter nbconvert --execute --ExecutePreprocessor.enabled=True --ExecutePreprocessor.timeout=50000 decision/PLP_ML/ML.ipynb'
            }
        }
    }
}
